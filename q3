#include <iostream>
using namespace std;

float max(float a,float b)
{ if(a>b)
   return a;
   else 
   return b;
}

float min(float a,float b)
{ if(a>b)
   return b;
   else 
   return a;
}

int main() {
    float a1,b1;
    cout<<"Enter any 2 numbers";
    cin>>a1>>b1;
    cout<<"\nMaximun number is "<< max(a1,b1);
    cout<<"\nMinimum number is  "<< min(a1,b1);
	return 0;
}
